{
  "name": "mongo-express",
  "version": "1.0.0",
  "main": "dist",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src",
    "dev": "nodemon -q -w './**' --ext 'ts,json' --exec 'yarn start'",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "format": "pretty-quick --pattern '**/*.{ts,tsx,js,jsx,json,md}'"
  },
  "keywords": [
    "mongodb",
    "express"
  ],
  "author": "floatrx <ifloatrx@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mongoose": "^5.11.97",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.521.0",
    "@aws-sdk/lib-storage": "^3.521.0",
    "aws-sdk": "^2.1565.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-rate-limit": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.2"
  }
}
